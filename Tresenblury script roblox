local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Tresenblury", HidePremium = false, IntroText = "Loading script...", SaveConfig = true, ConfigFolder = "OrionTest"})
OrionLib:MakeNotification({
	Name = "Welcome",
	Content = "Welcome to my script - MATTSUN9746 :D",
	Image = "rbxassetid://4483345998",
	Time = 5
})
local Tab2 = Window:MakeTab({
	Name = "Humanoid",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Tab = Window:MakeTab({
	Name = "Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab2:AddSection({
	Name = "Humanoid section"
})
local Section = Tab:AddSection({
	Name = "script section"
})
Tab:AddButton({
	Name = "JulHub",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/JulHubz/JulHub/main/JulHub"))()
  	end    
})
Tab:AddButton({
	Name = "Brookhaven R4D",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/M1ZZ001/BrookhavenR4D/main/Brookhaven%20R4D%20Script'))()
  	end    
})
Tab:AddButton({
	Name = "Ghost hub",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/GhostPlayer352/Test4/main/GhostHub'))()
  	end    
})
Tab:AddButton({
	Name = "Harlen hub",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/harlenscripts/HarlenHub/main/HarlenScripts"))()
  	end    
})
Tab:AddButton({
	Name = "pet simulator 99 script",
	Callback = function()
      		loadstring(game:HttpGet("https://scriptblox.com/raw/BACKROOMS-Pet-Simulator-99!-ZapHub-The-Best-PS99-Script-12975"))()
